{
  "expo": {
    "owner": "danimr",
    "name": "Crazy Pit Stop",
    "slug": "CpsApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ios-crazy-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/crazy-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#D2160F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.danimr.CpsApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ios-crazy-icon.png",
        "backgroundColor": "#D2160F"
      },
      "package": "com.danimr.CpsApp"
    },
    "web": {
      "favicon": "./assets/images/crazy-splash.png"
    },
    "extra": {
      "eas": {
        "projectId": "c5c66f02-7a95-432c-80bc-90067bd0a839"
      }
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/AcuminVariableConcept.otf"]
        }
      ]
    ]
  }
}
